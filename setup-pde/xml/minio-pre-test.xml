<service>
  <id>MinIO-PRE-Test</id>
  <name>MinIO-PRE-Test</name>
  <description>MinIO-PRE-Test</description>
  <executable>minio.exe</executable>
  <env name="MINIO_ACCESS_KEY" value="storage-access-key"/>
  <env name="MINIO_SECRET_KEY" value="storage-secret-key"/>
  <arguments>server --address :9003 "..\..\data\minio_pre_test"</arguments>
  <logmode>rotate</logmode>
  <startmode>Manual</startmode>
</service>