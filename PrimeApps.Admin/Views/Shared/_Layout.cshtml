    <!-- User -->
    <ul class="nav align-items-center d-md-none">
        <li class="nav-item">
            <a href class="nav-link nav-link-icon" href="@Url.Action("Reload", "Home")" class="btn-icon-clipboard" data-clipboard-text="" title="Reload organizations">
                <i class="fas fa-redo-alt"></i>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link nav-link-icon" href="@Url.Action("Logout", "Home")">
                <i class="fas fa-sign-out-alt"></i>
            </a>
        </li>
    </ul>
    <div class="collapse navbar-collapse" id="sidenav-collapse-main">
        <div class="navbar-collapse-header d-md-none">
            <div class="row">
                <div class="col collapse-brand">
                    <div class="row no-gutters align-items-center">
                        <div class="col-auto">
                            <div class="avatar-oval">
                                <i class="fas fa-user-alt"></i>
                                @* <img src="" alt="" /> *@
                            </div>
                        </div>
                        <div class="col pl-2">
                            @ViewBag.User.FirstName @ViewBag.User.LastName
                        </div>
                    </div>
                </div>
                <div class="col-auto collapse-close">
                    <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#sidenav-collapse-main" aria-controls="sidenav-main" aria-expanded="false" aria-label="Toggle sidenav">
                        <span></span>
                        <span></span>
                    </button>
                </div>
            </div>
        </div>
        <ul class="list-unstyled organisation-list">
            @if (organizations != null)
            {
                /*foreach (var organization in organizations)
                    {
                        <li class="nav-item">
                            <a class="nav-link " href="/examples/icons.html">
                                <i class="@(organization.Icon ?? "ni ni-planet") text-blue"></i> @organization.Name
                            </a>
                        </li>
                    }*/

                foreach (var organization in organizations)
                {
                    <li class="@(organization.Id.ToString() == ViewBag.ActiveOrganizationId ? "active" : "")">
                        <a href="@Url.Action("Index", "Home", new {id = organization.Id})">
                            <i class="@(@organization.Icon ?? "ni ni-planet")" style="background: @organization.Color"></i> <span>@organization.Label</span>
                        </a>
                    </li>
                }
            }
        </ul>
    </div>
</nav>

<div class="main-content">
    <!-- Navbar -->
    <nav class="navbar navbar-expand-md d-none d-md-flex" id="navbar-main">
        <div class="container-fluid">
            <!-- Brand -->
            <a class="h4 mb-0 text-uppercase" href="@Url.Action("Index", "Home")">Applications</a>
            <!-- User -->
            <ul class="navbar-nav align-items-center">
                <li class="nav-item pr-2">
                    <a href="@Url.Action("Reload", "Home")" data-clipboard-text="" title="Reload organizations">
                        <div class="avatar-oval">
                            <i class="fas fa-redo-alt"></i>
                        </div>
                    </a>
                </li>
                <li class="dropdown">
                    <a href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <div class="avatar-oval">
                            <i class="fas fa-user-alt"></i>
                            @* <img src="" alt="" /> *@
                        </div>
                    </a>
                    <div class="dropdown-menu dropdown-menu-arrow dropdown-menu-right ">
                        <div class="p-3">
                            <div class="row no-gutters align-items-center">
                                <div class="col-auto">
                                    <div class="avatar-oval">
                                        <i class="fas fa-user-alt"></i>
                                        @* <img src="" alt="" /> *@
                                    </div>
                                </div>
                                <div class="col pl-2">
                                    <h4 class="mb-0">@ViewBag.User.FirstName @ViewBag.User.LastName</h4>
                                    <small class="mb-0">@ViewBag.User.Email</small>
                                </div>
                            </div>
                        </div>

                        <div class="dropdown-divider"></div>
                        <a href="@Url.Action("Logout", "Home")" class="dropdown-item">
                            <i class="fas fa-sign-out-alt"></i>
                            <span>Logout</span>
                        </a>
                    </div>
                </li>
            </ul>
        </div>
    </nav>
    <!-- End Navbar -->
    @RenderBody()
</div>


<!-- Core -->
<script src="/js/plugins/jquery/jquery.min.js"></script>
<script src="/js/plugins/bootstrap/bootstrap.bundle.min.js"></script>
<script src="/js/plugins/clipboard/clipboard.min.js"></script>
<script src="/js/plugins/sweetalert2/sweetalert2.js"></script>
<!--  JS -->
<script src="/js/argon-dashboard.min.js"></script>


@RenderSection("Scripts", required: false)
</body>
</html>